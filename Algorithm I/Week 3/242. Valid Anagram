## 강민수 풀이

class Solution {
public:
    bool isAnagram(string s, string t) {
        unordered_map<char, int> map;
        for(auto c:s){
            map[c]++;
        }
        for(auto c:t){
            if(map.find(c) != map.end()){
                if(map[c] > 1) map[c]--;
                else map.erase(c); 
            }
            else return false;
            
        }
        return !map.size();
    }
};
