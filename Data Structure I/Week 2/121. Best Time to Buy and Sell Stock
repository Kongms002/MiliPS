### 강민수 풀이

class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int maxVal = 0, minVal = 10001;
        for (int i = 0; i < prices.size();i++){
            minVal = min(minVal, prices[i]);
            maxVal = max(maxVal, prices[i] - minVal);
        }
        return maxVal;
    }
};

Tp : O(n)/ Sp : O(1)
