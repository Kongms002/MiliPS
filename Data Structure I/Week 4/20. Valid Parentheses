### 강민수 풀이

class Solution {
public:
    bool isValid(string s) {
        stack<char> st;
        for(auto c:s){
            if (c == ')') {
                if (!st.empty() && st.top() == '(') st.pop();
                else return false;
            }
            else if (c == ']') {
                if (!st.empty() && st.top() == '[') st.pop();
                else return false;
            }
            else if (c == '}') {
                if (!st.empty() && st.top() == '{') st.pop();
                else return false;
            }
            else st.push(c);
        }
        return st.empty();
    }
};
