### 이원호 풀이 ###

class Solution {
public:
    bool canConstruct(string ransomNote, string magazine)
    {
        int a[26]={0,};
        for(int i=0; i<magazine.size(); i++)
            a[magazine[i]-97]++;
        for(int i=0; i<ransomNote.size(); i++)
        {
            a[ransomNote[i]-97]--;
            if(a[ransomNote[i]-97]<0)
                return false;
        }
        return true;
    }
};
